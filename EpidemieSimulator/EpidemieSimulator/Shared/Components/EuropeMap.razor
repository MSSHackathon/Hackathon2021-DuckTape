    private async Task SelectRegion(string key, int mode)
    {
        var args = new object[] { regionDataDict[key].MapPart, 1 };
        await Js.InvokeVoidAsync("getElementID", args);
        MapService.InvokeMapUpdate();

        if (!_pandamicStarted)
        {
            _pandamicStarted = true;
            await StartInfection(true);
        }
    }

    public string GetStateColor(string key)
    {
        var healthyPopulation = regionDataDict[key].Data.HealthyPopulation;
        var infectedPopulation = regionDataDict[key].Data.InfectedPopulation;
        var deceasedPopulation = regionDataDict[key].Data.DeceasedPopulation;
        var curedPopulation = regionDataDict[key].Data.CuredPopulation;

        if (healthyPopulation * 0.90 >= infectedPopulation)
        {
            //Green
            return "#60D394";
        }
        else if (healthyPopulation * 0.80 >= infectedPopulation && healthyPopulation * 0.90 < infectedPopulation)
        {
            //Orange
            return "#FFD97D";
        }
        else if (healthyPopulation * 0.80 < infectedPopulation && healthyPopulation > 0)
        {
            //Red
            return "#EE6055";
        }
        else if (curedPopulation * 0.05 > infectedPopulation)
        {
            //Light Blue
            return "#81dafc";
        }
        else if (curedPopulation * 0.50 > infectedPopulation)
        {
            //Dark Blue
            return "#0484b5";
        }
        else if (healthyPopulation * 0.50 < infectedPopulation)
        {
            //Dark red
            System.Diagnostics.Debug.WriteLine("Orange");
            return "#600000";
        }
        return string.Empty;
    }
