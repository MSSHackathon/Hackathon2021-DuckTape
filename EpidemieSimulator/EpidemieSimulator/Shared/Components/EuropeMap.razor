    public async Task StartInfection(bool first, string id = null)
    {
        var sir = new SIR();
        if (first)
        {
            id = await Js.InvokeAsync<string>("getSelectedEntry");
        }
        RegionData groundZeroData = regionDataDict[id].Data;
        regionDataDict[id].Data.runningPandemic = true;
        var data = sir.yieldCalcEverything(N: groundZeroData.Population, S: groundZeroData.Population - groundZeroData.InfectedPopulation, I: groundZeroData.InfectedPopulation + 1, R: 0, t: 0, b: 1.7, a: 0.1, scale: 0.125);

        await foreach (var item in data)
        {
            regionDataDict[id].Data.InfectedPopulation = Convert.ToInt64(item.I);
            regionDataDict[id].Data.HealthyPopulation = Convert.ToInt64(item.S);
            regionDataDict[id].Data.DeceasedPopulation = Convert.ToInt64(item.R * 0.025);
            regionDataDict[id].Data.CuredPopulation = Convert.ToInt64(item.R * 0.975);
            MapService.BaseData = regionDataDict.ToDictionary(pair => pair.Key, pair => pair.Value.Data);
            regionDataDict[id].Color = GetStateColor(id);
            await InvokeAsync(StateHasChanged);
            var randomNum = rnd.Next(0, regionDataDict[id].Data.Neighbours.Count);
            var neighbourData = regionDataDict[id].Data.Neighbours[randomNum];
            var neighbourKey = regionDataDict.Single(x => x.Value.Data.Name == neighbourData.Name).Key;
            var infectedPercentage = ((float)regionDataDict[id].Data.InfectedPopulation) / ((float)regionDataDict[id].Data.Population) * 100;
            if (rnd.Next(0, 100) <= infectedPercentage && regionDataDict[neighbourKey].Data.runningPandemic == false)
            {
                regionDataDict[neighbourKey].Data.runningPandemic = true;
                Task.Run(async () => { await StartInfection(false, neighbourKey); });
            }
        }
    }

